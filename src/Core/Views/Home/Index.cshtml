<div class="row">
    <div class="large-12 columns regular-background">
        <h1 class="title"><img src="@Url.Content("~/Content/logo.png")" alt="Vandelay Industries" /></h1>
    </div>
</div>
<div class="row">
    <div class="large-12 medium-12 columns regular-background">
        <nav class="main-nav small-6 small-centered column" ng-controller="NavigationCtrl">
            <a ng-repeat="link in links" style="white-space:nowrap" href="{{ link.url }}">{{ link.name }}</a>
        </nav>
    </div>
</div>
<div class="row main-content">
    <div class="large-12 columns" ng-view>
    </div>
</div>
